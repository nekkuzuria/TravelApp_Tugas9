<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_navigation"
    app:startDestination="@id/homepageFragment">

    <fragment
        android:id="@+id/homepageFragment"
        android:name="com.example.travelapp_tugas9.HomepageFragment"
        android:label="fragment_homepage"
        tools:layout="@layout/fragment_homepage" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.travelapp_tugas9.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/ticketFragment"
        android:name="com.example.travelapp_tugas9.TicketFragment"
        android:label="fragment_ticket"
        tools:layout="@layout/fragment_ticket" >
        <action
            android:id="@+id/action_ticketFragment_to_orderTicketFragment"
            app:destination="@id/orderTicketFragment" />
    </fragment>
    <fragment
        android:id="@+id/orderTicketFragment"
        android:name="com.example.travelapp_tugas9.OrderTicketFragment"
        android:label="fragment_order_ticket"
        tools:layout="@layout/fragment_order_ticket" />
</navigation>